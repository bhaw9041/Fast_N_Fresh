<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OrderExpensesController">
   <children>
      <BarChart fx:id="barChart" layoutX="79.0" layoutY="177.0" verticalGridLinesVisible="false">
        <xAxis>
          <CategoryAxis label="Date" side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" label="Order Amount" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="79.0" layoutY="44.0" mnemonicParsing="false" onAction="#goToCatalog" prefHeight="26.0" prefWidth="107.0" text="Back To Catalog" />
   </children>
</AnchorPane>
